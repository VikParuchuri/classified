                "description": "1-5 code correctness score."
            },
            "efficient": {
                "type": "integer",
                "description": "1-5 code efficiency score."
            },
            "readable": {
                "type": "integer",
                "description": "1-5 code readability score."
            },
            "complete": {
                "type": "integer",
                "description": "1-5 completeness score."
            },
            "written_by_expert": {
                "type": "integer",
                "description": "1-5 written by expert score."
            },
            "overall": {
                "type": "integer",
                "description": "1-5 overall score representing educational value of the code resource to a student."
            }
        },
        "required": ["correct", "efficient", "readable", "complete", "written_by_expert", "overall"]
